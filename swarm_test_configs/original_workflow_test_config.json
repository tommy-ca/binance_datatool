{
  "test_name": "Original Archive Collection Workflow Performance Test",
  "test_type": "traditional_workflow",
  "version": "2.0.0",
  "description": "Baseline performance test for traditional archive collection workflow",
  
  "workflow_config": {
    "workflow_type": "archive_samples",
    "markets": ["spot"],
    "symbols": ["BTCUSDT", "ETHUSDT"],
    "data_types": ["klines"],
    "intervals": ["1m", "5m", "1h"],
    "date_range": {
      "start_date": "2025-07-15",
      "end_date": "2025-07-15"
    },
    
    "enable_s3_direct_sync": false,
    "operation_mode": "traditional",
    "base_output_dir": "output/swarm-test-original-workflow",
    "dry_run": false,
    "verbose": true,
    
    "traditional_settings": {
      "max_parallel_downloads": 8,
      "batch_size": 50,
      "enable_batch_mode": true,
      "timeout_seconds": 300,
      "retry_count": 3
    },
    
    "s5cmd_settings": {
      "enable_s5cmd": true,
      "s5cmd_extra_args": ["--no-sign-request", "--retry-count=3", "--numworkers=8"],
      "part_size_mb": 50,
      "use_multipart": true
    }
  },
  
  "test_parameters": {
    "expected_file_count": 6,
    "test_iterations": 3,
    "warmup_runs": 1,
    "timeout_seconds": 600,
    "baseline_targets": {
      "typical_processing_time_seconds": 180,
      "max_memory_usage_mb": 1000,
      "min_success_rate_percent": 90,
      "operations_per_file": 5
    }
  },
  
  "monitoring": {
    "enable_performance_tracking": true,
    "enable_detailed_logging": true,
    "metrics_collection_interval": 5,
    "log_level": "INFO",
    "collect_system_metrics": true,
    "track_local_storage_usage": true
  },
  
  "validation": {
    "enable_checksum_validation": true,
    "enable_size_verification": true,
    "enable_local_file_verification": true,
    "validate_s5cmd_availability": true,
    "track_temporary_files": true
  },
  
  "metadata": {
    "test_environment": "swarm",
    "agent_type": "original-workflow-tester",
    "test_framework": "claude-flow-swarm",
    "created_date": "2025-07-20",
    "test_objectives": [
      "Establish baseline performance metrics",
      "Measure traditional workflow efficiency",
      "Track local storage requirements",
      "Document operation count per file",
      "Validate current success rates"
    ]
  }
}